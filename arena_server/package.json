{
  "name": "arena1",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run build:swagger build:css",
    "clean": "rimraf dist/*",
    "swagger": "tsoa spec",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build:app": "tsc",
    "build": "npm-run-all clean build:app copy-assets",
    "build:css": "sass src/public_scss/main.scss src/public/main.css --no-source-map",
    "build:swagger": "npm run swagger",
    "dev:start": "npm run tsstart",
    "dev": "npm-run-all --parallel dev:app dev:tools",
    "dev:tools": "npm-run-all --parallel dev:swagger dev:css",
    "dev:app": "nodemon --watch src -e ts --exec npm run dev:start",
    "dev:swagger": "nodemon --watch src --watch tsoa.json -e ts --exec npm run build:swagger",
    "dev:css": "sass --no-source-map --watch src/public_scss/main.scss src/public/main.css",
    "start": "node .",
    "tsstart": "ts-node src/index.ts",
    "test": "npx jest",
    "//initdb": "ts-node tools/initdb",
    "publish": "npm-run-all test build pub.deploy pub.open",
    "pub.deploy": "gcloud app deploy --quiet",
    "pub.log": "gcloud app logs tail -s default",
    "pub.open": "gcloud app browse"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-mutex": "^0.4.0",
    "build": "^0.1.4",
    "core-js": "^3.26.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-slow-down": "^1.5.0",
    "lodash": "^4.17.21",
    "materialize-css": "^1.0.0",
    "mongodb": "^4.12.1",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-http": "^0.3.0",
    "random-seed": "^0.3.0",
    "socket.io": "^4.5.4",
    "swagger-ui-express": "^4.6.0",
    "ts-retry": "^4.1.0",
    "tsoa": "^4.1.3",
    "typedi": "^0.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.190",
    "@types/materialize-css": "^1.0.11",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/passport": "^1.0.11",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-http": "^0.3.9",
    "@types/random-seed": "^0.3.3",
    "@types/shelljs": "^0.8.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-polyfill": "^3.8.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  }
}
